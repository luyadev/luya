{
	"name" : "luyadev/luya-module-admin",
	"description" : "Yii2 LUYA Admin",
	"type" : "luya-module",
	"keywords" : ["php", "luya", "module", "admin", "yii2", "yii", "administration", "angular", "ngrest", "manage", "panel"],
	"license": "MIT",
	"homepage" : "http://luya.io",
	"authors" : [
		{
			"name" : "Basil",
			"email" : "git@nadar.io",
			"homepage" : "https://github.com/nadar"
		}
	],
	"support" : {
		"issues" : "https://github.com/luyadev/luya/issues"
	},
	"require" : {
	    "yiisoft/yii2-imagine": "~2.0.0",
	    "cebe/markdown": "~1.0.0 | ~1.1.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~5.0",
		"luyadev/luya" : "^1.0@dev",
		"satooshi/php-coveralls": "~1.0"
	},
	"autoload" : {
		"psr-4" : {
			"admin\\" : "src/",
			"admintests\\" : "tests/"
		}
	},
	"autoload-dev" : {
       "psr-4" : {
           "admintests\\" : "tests/"
       }
    },
	"extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
