{
    "minimum-stability" : "RC",
	"name" : "luyadev/luya-module-admin",
	"description" : "Administration core module for all LUYA admin modules",
	"type" : "luya-core",
	"keywords" : ["php", "luya", "module", "admin", "yii2", "yii", "administration", "angular", "ngrest", "manage", "panel"],
	"license": "MIT",
	"homepage" : "http://luya.io",
	"authors" : [
		{
			"name" : "Basil Suter",
			"email" : "git@nadar.io",
			"homepage" : "https://github.com/nadar"
		}
	],
	"support" : {
		"issues" : "https://github.com/luyadev/luya/issues"
	},
	"require" : {
	    "yiisoft/yii2-imagine": "~2.1.0",
	    "flowjs/flow-php-server": "^1.0"
	},
	"require-dev" : {
		"luyadev/luya-testsuite" : "^1.0@dev",
		"luyadev/luya-module-cms" : "^1.0@dev",
		"satooshi/php-coveralls": "~1.0"
	},
	"autoload" : {
		"psr-4" : {
			"luya\\admin\\" : "src/"
		}
	},
	"autoload-dev" : {
       "psr-4" : {
           "admintests\\" : "tests/"
       }
    },
	"extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "fxp-asset" : {
            "pattern-skip-version": "(-build|-patch)"
        }   
    }
}
